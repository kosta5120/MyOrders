<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/Sass/OrdersPage.min.css")
    @Styles.Render("~/Content/bootstrap.min.css")
    @Styles.Render("~/Content/Site.css")
    @Styles.Render("~/Content/Sass/Header.min.css")
    @Styles.Render("~/Content/dataTables.bootstrap.min.css")
    @Styles.Render("~/Content/Sass/Error.min.css")
</head>
<body>

    @{
        Html.RenderPartial("_HeaderNavBar");
    }

    <div>
        @RenderBody()
    </div>
    @RenderSection("scripts", required: false)

    @Scripts.Render("~/Scripts/jquery-3.4.1.min.js")
    @Scripts.Render("~/Scripts/bootstrap.min.js")
    @*@Scripts.Render("~/Scripts/bootstrap.js")*@
    @Scripts.Render("~/Scripts/Orders.js")
    @Scripts.Render("~/Scripts/datatables.min.js")
    @Scripts.Render("~/Scripts/jquery.dataTables.min.js")
    @Scripts.Render("~/Scripts/dataTables.bootstrap.min.js")
</body>
</html>
